{
  "name": "js-templates",
  "displayName": "JS Templates",
  "description": "Template file generator in JS",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createTemplate"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "title": "JS Templates",
      "properties": {
        "js-templates.templateFilePaths": {
          "type": "array",
          "default": [
            "src/__templates__",
            "js-templates/__templates__"
          ],
          "description": "The locations of the folders containing your template folders."
        }
      }
    },
    "commands": [
      {
        "command": "extension.createTemplate",
        "title": "Create From Template"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "extension.createTemplate",
          "group": "navigation@10"
        }
      ]
    }
  },
  "scripts": {
    "test": "node ./test/runTest.js",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^10.2.0",
    "typescript": "^3.3.1",
    "vscode": "^1.1.35",
    "vscode-test": "^1.0.2"
  }
}
